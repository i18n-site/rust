set -e
DIR=`pwd`

if [ -f "../.envrc" ]; then
source_up
fi

export RUSTFLAGS='--cfg reqwest_unstable -C target-feature=+aes -Z threads=8' 
export RUST_LOG=debug,supervisor=warn,hyper=warn,rustls=warn,h2=warn,tower=warn,h3=warn,quinn_udp=warn,quinn_proto=warn,watchexec=warn,globset=warn,hickory_proto=warn,hickory_resolver=warn,aws_smithy_runtime=warn,aws_sdk_s3=warn
export RUST_BACKTRACE=short

bin=$DIR/.direnv/bin
PATH_add $bin
bun_i .
